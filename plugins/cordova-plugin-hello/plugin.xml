<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-hello" version="0.0.1"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>hello</name>
    <js-module name="hello" src="www/hello.js">
        <clobbers target="cordova.plugins" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="hello">
                <param name="android-package" value="cordova.plugin.hello.hello" />
            </feature>
        </config-file>
        <config-file parent="/manifest/application" target="AndroidManifest.xml">
            <meta-data android:name="com.google.ar.core" android:value="required" />

            <activity android:name="com.dennis.sceneformanim.HelloArActivity" android:label="@string/app_name" android:theme="@style/Theme.AppCompat">
            </activity>
        </config-file>

        <source-file src="src/android/com/dennis/sceneformanim/HelloArActivity.java" target-dir="src/cordova/plugin/com/google/dennis/sceneformanim/" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-hdpi/ic_launcher_round.png" target="res/mipmap-hdpi/ic_launcher_round.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-hdpi/ic_launcher.png" target="res/mipmap-hdpi/ic_launcher.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/values/strings.xml" target="res/values/strings.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/values/styles.xml" target="res/values/styles.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/values/colors.xml" target="res/values/colors.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/raw.sfb" target="res/raw.sfb" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/layout/activity_main.xml" target="res/layout/activity_main.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-xhdpi/ic_launcher_round.png" target="res/mipmap-xhdpi/ic_launcher_round.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-xhdpi/ic_launcher.png" target="res/mipmap-xhdpi/ic_launcher.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/raw/cangrejo.sfb" target="res/raw/cangrejo.sfb" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/raw/cangrejo_talk.sfb" target="res/raw/cangrejo_talk.sfb" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-anydpi-v26/ic_launcher.xml" target="res/mipmap-anydpi-v26/ic_launcher.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-anydpi-v26/ic_launcher_round.xml" target="res/mipmap-anydpi-v26/ic_launcher_round.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/drawable/ic_baseline_play_arrow_24.xml" target="res/drawable/ic_baseline_play_arrow_24.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/drawable/ic_launcher_background.xml" target="res/drawable/ic_launcher_background.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-mdpi/ic_launcher_round.png" target="res/mipmap-mdpi/ic_launcher_round.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-mdpi/ic_launcher.png" target="res/mipmap-mdpi/ic_launcher.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/drawable-v24/ic_launcher_foreground.xml" target="res/drawable-v24/ic_launcher_foreground.xml" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-xxhdpi/ic_launcher_round.png" target="res/mipmap-xxhdpi/ic_launcher_round.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-xxhdpi/ic_launcher.png" target="res/mipmap-xxhdpi/ic_launcher.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-xxxhdpi/ic_launcher_round.png" target="res/mipmap-xxxhdpi/ic_launcher_round.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-xxxhdpi/ic_launcher.png" target="res/mipmap-xxxhdpi/ic_launcher.png" />
        <resource-file src="src/android/com/dennis/sceneformanim/res/mipmap-xxxhdpi/ic_launcher.png" target="res/mipmap-xxxhdpi/ic_launcher.png" />
        <resource-file src="src/android/assets/museum.mp3" target="assets/museum.mp3" />
        <resource-file src="src/android/assets/top.html" target="assets/top.html" />
        <resource-file src="src/android/assets/bottom.html" target="assets/bottom.html" />
        <resource-file src="src/android/assets/subtitles.js" target="assets/subtitles.js" />

        <source-file src="src/android/hello.java" target-dir="src/cordova/plugin/hello/hello" />
        <framework src="src/android/build.gradle" custom="true" type="gradleReference" />
    </platform>
</plugin>
